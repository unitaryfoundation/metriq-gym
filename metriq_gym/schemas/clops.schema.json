{
    "$id": "metriq-gym/clops.schema.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",  
    "title": "CLOPS",
    "type": "object",
    "properties": {
      "benchmark_name": {
        "type": "string",
        "const": "CLOPS"
      },
      "width": {
        "type": "integer",
        "default": 1000,
        "minimum": 1,
        "examples": [1000]
      },
      "num_layers": {
        "type": "integer",
        "default": 1000,
        "minimum": 1,
        "examples": [1000]
      },
      "num_circuits": {
        "type": "integer",
        "default": 100,
        "minimum": 1,
        "examples": [100]
      },
      "shots": {
        "type": "integer",
        "default": 1000,
        "minimum": 1,
        "examples": [1000]
      }
    },
    "required": ["benchmark_name"]
  }
  