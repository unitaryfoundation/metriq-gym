{
  "$id": "metriq-gym/bseq.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",  
  "title": "BSEQ",
  "type": "object",
  "properties": {
    "benchmark_name": {
      "type": "string",
      "const": "BSEQ"
    },
    "shots": {
      "type": "integer",
      "default": 1000,
      "minimum": 1,
      "examples": [1000]
    }
  },
  "required": ["benchmark_name"]
}
